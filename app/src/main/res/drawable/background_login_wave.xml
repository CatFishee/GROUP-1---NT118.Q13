<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="100dp"
    android:height="100dp"
    android:viewportWidth="100"
    android:viewportHeight="100">

    <path
        android:pathData="M 0,20 Q 25,0 50,20 T 100,20 L 100,100 L 0,100 Z">

        <aapt:attr name="android:fillColor">
            <gradient
                android:startX="50"
                android:startY="0"
                android:endX="50"
                android:endY="100"
                android:type="linear">
                <item android:color="@color/login_gradient_start" android:offset="0.0"/>
                <item android:color="@color/login_gradient_end" android:offset="1.0"/>
            </gradient>
        </aapt:attr>

    </path>

</vector>